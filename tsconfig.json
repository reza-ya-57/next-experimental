{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "noImplicitAny": false,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": false,
    "module": "commonjs",
    "outDir": "dist",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true ,
    "baseUrl": "/src",
    "paths": {
      "@/styles/*":["src/styles/*"] ,
    }

  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "src/pages/upload/index.js" , "server/server.ts"],
  "exclude": ["node_modules"]
}
